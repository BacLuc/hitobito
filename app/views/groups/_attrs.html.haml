- @sheets = GroupSheet.new(self, @group)

=render 'groups/tabs'

#main.row-fluid
  %article.span7

    = render 'contactable/show', contactable: entry, only_public: cannot?(:show_details, @group)

    - if @contact
      %h4 Kontaktperson
      = render 'contactable/show', contactable: @contact, only_public: true, with_name: true

    = render_extensions :attrs

  = render 'child_groups'
