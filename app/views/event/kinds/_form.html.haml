= render 'shared/admin_tabs'

= entry_form(buttons_bottom: false, 
             cancel_url: event_kinds_path) do |f|
             
   = f.labeled_input_fields :label, :short_name
   = f.labeled_input_field :minimum_age, help_inline: 'Jahre'
   = f.labeled_input_fields :precondition_ids, :qualification_kind_ids
   = f.labeled_input_field :prolongation_ids, help: 'Mit <Ctrl> + Klick können mehrere Qualifikationsarten ausgewählt werden.'
